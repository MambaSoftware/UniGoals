{
    "name": "unigoals",
    "version": "0.0.6",
    "author": "MambaSoftware",
    "description": "UniGoals is modern day University course unit tracking utility designed to allow you to be able to constantly know where you currently stand on your course.",
    "scripts": {
        "build:hot": "webpack-dashboard -- webpack-dev-server --content-base ./public -d --hot --history-api-fallback true --config ./webpack.config.development.js",
        "build:dev": "webpack -d --progress --watch --config ./webpack.config.development.js",
        "build": "npm run lint && webpack -p --progress --config ./webpack.config.production.js",
        "lint": "eslint -c ./.eslintrc.js --ext .js,.jsx --ignore-path ../../.gitignore .",
        "lint:fix": "eslint -c ./.eslintrc.js --ext .js,.jsx --ignore-path ../../.gitignore . --fix",
        "deploy": "npm run build && firebase deploy"
    },
    "license": "MIT",
    "dependencies": {
        "@blueprintjs/core": "^1.33.0",
        "@blueprintjs/labs": "^0.13.0",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-preset-env": "^1.6.1",
        "babel-preset-react": "^6.24.1",
        "body-parser": "^1.18.2",
        "css-loader": "^0.28.7",
        "express": "^4.16.2",
        "firebase": "^4.7.0",
        "history": "^4.7.2",
        "less": "^2.7.3",
        "less-loader": "^4.0.5",
        "lodash": "^4.17.4",
        "prop-types": "^15.6.0",
        "react": "^16.2.0",
        "react-addons-css-transition-group": "^15.6.2",
        "react-dom": "^16.2.0",
        "react-google-login": "^3.0.2",
        "react-redux": "^5.0.6",
        "react-router": "^4.2.0",
        "react-router-dom": "^4.2.2",
        "react-router-hash-link": "^1.1.1",
        "react-router-redux": "^5.0.0-alpha.8",
        "recharts": "^1.0.0-beta.5",
        "redux": "^3.7.2",
        "style-loader": "^0.18.2",
        "superagent": "^3.8.1"
    },
    "devDependencies": {
        "babel-eslint": "^8.0.3",
        "eslint": "^4.12.1",
        "eslint-config-airbnb": "^16.1.0",
        "eslint-plugin-import": "^2.8.0",
        "eslint-plugin-jsx-a11y": "^6.0.2",
        "eslint-plugin-react": "^7.5.1",
        "eslint-plugin-sort-requires": "^2.1.0",
        "webpack": "^3.9.1",
        "webpack-dashboard": "^1.0.2",
        "webpack-dev-server": "^2.9.5"
    }
}
