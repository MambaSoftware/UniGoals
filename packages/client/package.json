{
  "name": "unigoals",
  "version": "0.4.1",
  "author": "MambaSoftware",
  "description": "UniGoals is modern day University course unit tracking utility designed to allow you to be able to constantly know where you currently stand on your course.",
  "scripts": {
    "build:hot": "webpack-dev-server -d --config ./webpack.config.development.js",
    "build:dev": "webpack -d --watch --config ./webpack.config.development.js",
    "build": "npm run lint && webpack -p --progress --env.production --config ./webpack.config.production.js",
    "lint": "eslint -c ./.eslintrc.js --ext .js,.jsx --ignore-path ../../.gitignore .",
    "lint:fix": "eslint -c ./.eslintrc.js --ext .js,.jsx --ignore-path ../../.gitignore . --fix",
    "deploy": "npm run build && firebase deploy"
  },
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^1.4.1",
    "@material-ui/icons": "^1.1.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "classnames": "^2.2.6",
    "firebase": "^4.11.0",
    "firebase-bolt": "^0.8.2",
    "firebase-tools": "^4.0.0",
    "history": "^4.7.2",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-hash-link": "^1.2.0",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-sort-requires": "^2.1.0",
    "webpack": "^4.16.2",
    "webpack-cli": "^2.1.5",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-server": "^3.1.5"
  }
}
